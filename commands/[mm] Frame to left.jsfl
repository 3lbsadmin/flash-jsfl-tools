/** * Frame to left * * @author: Mediamonks - http://www.mediamonks.com * @author: Mark Knol  - http://blog.stroep.nl */ var doc = fl.getDocumentDOM();var timeline = fl.getDocumentDOM().getTimeline();if (!doc) {	alert("Please open or create a flashfile.");} else {	FrameToLeft();}function FrameToLeft(){	if (timeline.currentFrame > 0)	{		timeline.removeFrames(timeline.currentFrame - 1);		timeline.insertFrames(1, false, timeline.currentFrame);	}}