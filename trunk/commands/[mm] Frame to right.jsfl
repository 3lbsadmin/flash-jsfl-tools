/** * Frame to right *   * @author: Mediamonks - http://www.mediamonks.com * @author: Mark Knol  - http://blog.stroep.nl */ var doc = fl.getDocumentDOM();var timeline = fl.getDocumentDOM().getTimeline();if (!doc) {	alert("Please open or create a flashfile.");} else {	FrameToRight();}function FrameToRight(){	if (timeline.currentFrame < timeline.frameCount - 1)	{		timeline.removeFrames(timeline.currentFrame);		timeline.insertFrames(1, false, timeline.currentFrame-1);		timeline.setSelectedFrames(timeline.currentFrame+2, timeline.currentFrame+3);	}}