var success = true;var log = "";var docs = fl.documents;var currentDoc = fl.getDocumentDOM();if (confirm("Compile all open documents?")){	for (var i = 0; i < docs.length; i++) 	{		var aDoc = docs[i];		aDoc.publish();				var saveFileURI = fl.configURI + aDoc.name + ".errors"		fl.compilerErrors.save(saveFileURI);				var contents = FLfile.read(saveFileURI);				FLfile.remove(saveFileURI);				if (contents.split("\n").length > 1 || contents.split("\r").length > 1) 		{			log += (aDoc.name + " publish error!\n");			success = false;			break;		}		else		{			log += (aDoc.name + " published\n");		}	}	log += ("\n\n******************************************\nTotal movies published: " + i + "/" + docs.length + " open documents");	fl.outputPanel.trace(log);}